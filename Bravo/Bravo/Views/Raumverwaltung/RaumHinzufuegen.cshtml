
@{
    ViewBag.Title = "RaumHinzufuegen";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="navbar_side_child navbar-inverse navbar-fixed-side-child">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
    </div>
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            <li>@Html.ActionLink("Übersicht Räume", "Index", "Raumverwaltung")</li>
            <li>@Html.ActionLink("Raum hinzufügen", "RaumHinzufuegen", "Raumverwaltung")</li>
        </ul>
    </div>
</div>


<h3>Raum hinzufügen</h3>

<div id="content">
    <table>
        <tr><td>Bezeichnung</td><td><input id="bezeichnung" type="text" /></td></tr>
        <tr><td>Notiz</td><td><input id="notiz" type="text" /></td></tr>
    </table>


    <br /><br /><br />
    <input id="zurücksetzen" type="reset" value="Zurücksetzen" class="btn btn-primary" />
    <input id="erstellen" type="submit" value="Erstellen" class="btn btn-primary" />
    <br /><br />
    <input id="zurueck" type="submit" value="Zurück" class="btn btn-primary" />
</div>